{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    }
  ],
  "headers": [],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [
      {
        "source": "/api/:path*",
        "destination": "https://clean-subtitle-tsrxh.ondigitalocean.app/:path*",
        "regex": "^/api(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?(?:/)?$"
      },
      {
        "source": "/docs",
        "destination": "http://127.0.0.1:8000/docs",
        "regex": "^/docs(?:/)?$"
      },
      {
        "source": "/openapi.json",
        "destination": "http://127.0.0.1:8000/openapi.json",
        "regex": "^/openapi\\.json(?:/)?$"
      }
    ],
    "fallback": []
  },
  "dynamicRoutes": [],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/(?:/)?$"
    },
    {
      "page": "/_not-found",
      "regex": "^/_not\\-found(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/_not\\-found(?:/)?$"
    },
    {
      "page": "/app",
      "regex": "^/app(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/app(?:/)?$"
    },
    {
      "page": "/authentication",
      "regex": "^/authentication(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/authentication(?:/)?$"
    },
    {
      "page": "/docs",
      "regex": "^/docs(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/docs(?:/)?$"
    },
    {
      "page": "/errors",
      "regex": "^/errors(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/errors(?:/)?$"
    },
    {
      "page": "/favicon.ico",
      "regex": "^/favicon\\.ico(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/favicon\\.ico(?:/)?$"
    },
    {
      "page": "/health",
      "regex": "^/health(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/health(?:/)?$"
    },
    {
      "page": "/universal",
      "regex": "^/universal(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/universal(?:/)?$"
    },
    {
      "page": "/validate",
      "regex": "^/validate(?:/)?$",
      "routeKeys": {},
      "namedRegex": "^/validate(?:/)?$"
    }
  ],
  "dataRoutes": [],
  "rsc": {
    "header": "RSC",
    "varyHeader": "RSC, Next-Router-State-Tree, Next-Router-Prefetch, Next-Router-Segment-Prefetch",
    "prefetchHeader": "Next-Router-Prefetch",
    "didPostponeHeader": "x-nextjs-postponed",
    "contentTypeHeader": "text/x-component",
    "suffix": ".rsc",
    "prefetchSuffix": ".prefetch.rsc",
    "prefetchSegmentHeader": "Next-Router-Segment-Prefetch",
    "prefetchSegmentSuffix": ".segment.rsc",
    "prefetchSegmentDirSuffix": ".segments"
  },
  "rewriteHeaders": {
    "pathHeader": "x-nextjs-rewritten-path",
    "queryHeader": "x-nextjs-rewritten-query"
  }
}